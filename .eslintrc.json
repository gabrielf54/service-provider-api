{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn",
      "no-debugger": "warn",
          "no-empty": "warn",
          "no-extra-semi": "error",
          "no-undef": "error",
          "no-unreachable": "error",
          "dot-notation": "warn",
          "no-var": "warn",
                  "prefer-const": "warn"
    }
  }